---
import { Layout } from "tractstack:layouts";
import { CodeHook } from "tractstack:components";
import { setupTurso } from "tractstack";

await setupTurso({
  url: import.meta.env.TURSO_DATABASE_URL,
  authToken: import.meta.env.TURSO_AUTH_TOKEN,
});
---

<Layout title={`test`}>
  <CodeHook target={`hero`} />
</Layout>
